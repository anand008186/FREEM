{
  "canisters": {
    "freem_backend": {
      "main": "src/freem_backend/main.mo",
      "type": "motoko"
    },
    "promise_manager": {
      "main": "src/freem_backend/root_pool/main.mo",
      "type": "motoko"
    },
    "freem_frontend": {
      "entry_point": "src/freem_frontend/src/index.html",
      "dependencies": [
        "freem_backend"
      ],
      "source": [
        "src/freem_frontend/dist"
      ],
      "type": "assets",
      "workspace": "freem_frontend"
    }
  },
  "defaults": {
    "build": {
      "args": "--hide-warnings",
      "packtool": "mops sources"
    }
  },
  "output_env_file": ".env",
  "version": 1
}